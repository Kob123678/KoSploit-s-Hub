local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("KoSploit's Hub (Discontinued)", "Ocean")

-- Function to shorten button creation
local function createButton(section, label, tooltip, script)
    section:NewButton(label, tooltip, function()
        loadstring(game:HttpGet(script))()
    end)
end

-- Main Menu
local Tab = Window:NewTab("MAIN MENU")

local Toggle = Tab:NewSection("Toggle UI")
Toggle:NewKeybind("Toggle UI", "Click to change keybinds", Enum.KeyCode.LeftAlt, Library.ToggleUI)

local Server = Tab:NewSection("Server")
createButton(Server, "Rejoin", "Click to rejoin", "https://pastebin.com/raw/6wdd37J7")
createButton(Server, "Server Hop", "Click to hop server", "https://pastebin.com/raw/fuRgG35y")
createButton(Server, "Server Hop (Join Lowest Player Server)", "Click to hop server", "https://pastebin.com/raw/ZRpw2BDy")

